process {
    executor = 'pbs'
    memory ='64G'
    cpus = 2
    time ='0h 30m'
   
    withLabel: 'med' {
        executor = 'pbs'
        memory ='64G'
        time = '1h'
        cpus = 4
    }
    withLabel: 'high' {
        executor = 'pbs'
        time = '1h'
        memory ='64G'
        clusterOptions = { "-l nodes=4:ppn=2:gpus=2" }
    }
}


includeConfig "$projectDir/paramsElena.config"
singularity.enabled = true



